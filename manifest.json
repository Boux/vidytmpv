{

  "manifest_version": 3,
  "name": "vidytmpv",
  "version": "1.1.0",

  "description": "Adds button on youtube video page to open it in mpv",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "all_frames": true,
      "js": ["vidytmpv.js"],
      "css": ["style.css"]
    }
  ]

}
